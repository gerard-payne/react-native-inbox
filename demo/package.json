{
  "name": "DemoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx @react-native-community/cli run-android",
    "ios": "npx @react-native-community/cli run-ios",
    "lint": "eslint .",
    "start": "npx @react-native-community/cli start",
    "test": "jest",
    "build:android": "cd ../android && ./gradlew assembleDebug",
